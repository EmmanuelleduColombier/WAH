<div class="container mt-4">
  <div class="form-wrapper" style="width: 400px; margin: 10px auto;">
    <%= simple_form_for [ @booking, @review ] do |f| %>
      <%= f.input :content %>
      <%= f.input :rating %>
      <%= f.submit "Submit review", class: "btn btn-orange" %>
    <% end %>
  </div>
</div>
